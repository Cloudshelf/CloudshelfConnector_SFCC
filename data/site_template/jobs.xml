<?xml version="1.0" encoding="UTF-8"?>
<jobs xmlns="http://www.demandware.com/xml/impex/jobs/2015-07-01">
    <job job-id="exportStoresToCloudShelf" priority="0">
        <description/>
        <parameters/>
        <split sequential="false">
            <contexts template-ref="template_28">
                <all-storefront-sites/>
            </contexts>
            <flow-template template-id="template_28">
                <step step-id="exportLocations" type="custom.int_cloudshelf.exportLocationsToCloudShelf" enforce-restart="false">
                    <description/>
