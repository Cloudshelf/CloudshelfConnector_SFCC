                        <parameter name="jobMode">FULL</parameter>
                    </parameters>
                </step>
            </flow-template>
        </split>
        <rules/>
        <triggers>
            <run-once enabled="false">
                <date>2024-01-16Z</date>
                <time>15:57:00.000Z</time>
            </run-once>
        </triggers>
