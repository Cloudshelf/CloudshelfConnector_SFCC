<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="cloudshelf.http.graphql.credentials">
        <url>https://ingestapi.cloudshelf.ai/graphql</url>
        <user-id/>
        <password/>
    </service-credential>

    <service-profile service-profile-id="cloudshelf.http.graphql">
        <timeout-millis>15000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service service-id="cloudshelf.http.graphql">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>cloudshelf-graphql</log-prefix>
        <comm-log-enabled>true</comm-log-enabled>
        <force-prd-enabled>false</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>cloudshelf.http.graphql</profile-id>
        <credential-id>cloudshelf.http.graphql.credentials</credential-id>
    </service>

</services>